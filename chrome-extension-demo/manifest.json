{
    "name": "chaojibiaoge print",
    "version": "1.2.0",
    "manifest_version": 2,
    "description": "chaojibiaoge excel print",
    "update_url": "http://0.0.0.0",
    "icons":
    {
        "128": "icon.png"
    },
    "web_accessible_resources": [
        "logo.svg",
        "template.css"
    ],
    "permissions": ["*://chaojibiaoge.com/*"],
    "browser_action":
    {
        "default_title": "",
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "content_scripts": [
        {
            "matches": ["*://chaojibiaoge.com/*"],
            "js": ["chaojibiaoge-print.js"]
        }
    ]
}
